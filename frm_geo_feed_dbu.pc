        }
        gzFpIgn = fopen(fname, "w");
        strcpy(gszRejFname, fname);
    }

    if ( gzFpIgn != NULL ) {
        fprintf(gzFpIgn, rec_inf);
    }
}


void procWrtOuput(char out_ini[][SIZE_ITEM_L], char comm_ini[][SIZE_ITEM_L])
{
    wrtCellArea(out_ini, comm_ini);
    wrtCellSet(out_ini, comm_ini);
